<?xml version="1.0" encoding="UTF-8"?>
<window>
  <defaultcontrol>13</defaultcontrol>
  <include>16x9_xPos_Relocation</include>
  <include>Window_OpenClose_Animation_Zoom</include>
  <coordinates>
    <posx>240</posx>
    <posy>60</posy>
  </coordinates>
  <controls>
    <control type="group">
      <posx>0</posx>
      <posy>0</posy>
      <animation effect="fade" time="200">WindowOpen</animation>
      <animation effect="fade" time="200">WindowClose</animation>
      <control type="image">
        <posx>0</posx>
        <posy>0</posy>
        <width>800</width>
        <height>600</height>
        <texture border="40">DialogBack.png</texture>
      </control>
      <control type="image">
        <posx>0</posx>
        <posy>0</posy>
        <width>800</width>
        <height>600</height>
        <texture border="40">DialogFront.png</texture>
      </control>
      <!-- Title -->
      <control type="label" id="1">
        <description>Token Name</description>
        <posx>40</posx>
        <posy>30</posy>
        <width>720</width>
        <height>30</height>
        <align>center</align>
        <aligny>center</aligny>
        <font>font28_title</font>
        <textcolor>blue</textcolor>
        <label>Token Name</label>
      </control>
      <!-- Token Image -->
      <control type="image" id="2">
        <description>Token Image</description>
        <posx>40</posx>
        <posy>80</posy>
        <width>300</width>
        <height>200</height>
        <aspectratio>keep</aspectratio>
        <texture>DefaultAddonNone.png</texture>
        <bordertexture border="8">ThumbShadow.png</bordertexture>
        <bordersize>8</bordersize>
      </control>
      <!-- Token ID Label -->
      <control type="label" id="3">
        <description>Token ID Label</description>
        <posx>360</posx>
        <posy>80</posy>
        <width>200</width>
        <height>30</height>
        <align>right</align>
        <aligny>center</aligny>
        <font>font13</font>
        <textcolor>grey2</textcolor>
        <label>Token ID:</label>
      </control>
      <!-- Token ID Value -->
      <control type="label" id="4">
        <description>Token ID Value</description>
        <posx>370</posx>
        <posy>80</posy>
        <width>390</width>
        <height>30</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>font13</font>
        <textcolor>white</textcolor>
        <label>-</label>
      </control>
      <!-- Content ID Label -->
      <control type="label" id="5">
        <description>Content ID Label</description>
        <posx>360</posx>
        <posy>120</posy>
        <width>200</width>
        <height>30</height>
        <align>right</align>
        <aligny>center</aligny>
        <font>font13</font>
        <textcolor>grey2</textcolor>
        <label>Content ID:</label>
      </control>
      <!-- Content ID Value -->
      <control type="label" id="6">
        <description>Content ID Value</description>
        <posx>370</posx>
        <posy>120</posy>
        <width>390</width>
        <height>30</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>font13</font>
        <textcolor>white</textcolor>
        <label>-</label>
      </control>
      <!-- Type Label -->
      <control type="label" id="7">
        <description>Type Label</description>
        <posx>360</posx>
        <posy>160</posy>
        <width>200</width>
        <height>30</height>
        <align>right</align>
        <aligny>center</aligny>
        <font>font13</font>
        <textcolor>grey2</textcolor>
        <label>Type:</label>
      </control>
      <!-- Type Value -->
      <control type="label" id="8">
        <description>Type Value</description>
        <posx>370</posx>
        <posy>160</posy>
        <width>390</width>
        <height>30</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>font13</font>
        <textcolor>white</textcolor>
        <label>-</label>
      </control>
      <!-- Acquired Label -->
      <control type="label" id="9">
        <description>Acquired Label</description>
        <posx>360</posx>
        <posy>200</posy>
        <width>200</width>
        <height>30</height>
        <align>right</align>
        <aligny>center</aligny>
        <font>font13</font>
        <textcolor>grey2</textcolor>
        <label>Acquired:</label>
      </control>
      <!-- Acquired Value -->
      <control type="label" id="10">
        <description>Acquired Value</description>
        <posx>370</posx>
        <posy>200</posy>
        <width>390</width>
        <height>30</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>font13</font>
        <textcolor>white</textcolor>
        <label>-</label>
      </control>
      <!-- Description Label -->
      <control type="label" id="11">
        <description>Description Label</description>
        <posx>40</posx>
        <posy>300</posy>
        <width>720</width>
        <height>30</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>font13</font>
        <textcolor>grey2</textcolor>
        <label>Description:</label>
      </control>
      <!-- Description Value -->
      <control type="textbox" id="12">
        <description>Description Value</description>
        <posx>40</posx>
        <posy>330</posy>
        <width>720</width>
        <height>180</height>
        <font>font13</font>
        <textcolor>white</textcolor>
        <label>-</label>
        <autoscroll time="3000" delay="4000" repeat="5000">true</autoscroll>
      </control>
      <!-- Close button -->
      <control type="button" id="13">
        <description>Close</description>
        <posx>295</posx>
        <posy>530</posy>
        <width>210</width>
        <height>50</height>
        <align>center</align>
        <aligny>center</aligny>
        <font>font25</font>
        <label>186</label>
        <onleft>13</onleft>
        <onright>13</onright>
        <onup>13</onup>
        <ondown>13</ondown>
      </control>
    </control>
  </controls>
</window> 